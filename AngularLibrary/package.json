{
  "name": "angular-library",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:lib": "ng build lib",
    "build:ec": "ng build ec --aot --build-optimizer --common-chunk --vendor-chunk --optimization --progress --base-href=/ec-demo/",
    "build:crm": "ng build crm --base-href=/assets/angular/crm/",
    "build:cms": "ng build cms --base-href=/assets/angular/cms/",
    "build:mod": "ng build mod --aot --build-optimizer --common-chunk --vendor-chunk --optimization --progress",
    "build:brand": "ng build brand --base-href=/assets/angular/brand/",
    "start:ec": "ng serve ec --open",
    "start:crm": "ng serve crm --open",
    "start:cms": "ng serve cms --open",
    "start:brand": "ng serve brand --open",
    "start:mod": "ng serve mod --open",
    "build:mod:enhelper": "ng build mod --aot --build-optimizer --common-chunk --vendor-chunk --optimization --progress --base-href=/en-word-hero/",
    "build:mod:pal": "ng build mod --aot --build-optimizer --common-chunk --vendor-chunk --optimization --progress --base-href=/pal-tool/",
    "build:mod:memory": "ng build mod --aot --build-optimizer --common-chunk --vendor-chunk --optimization --progress --base-href=/assets/angular/mod/memory-card/"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^16.0.0",
    "@angular/animations": "^16.1.3",
    "@angular/cdk": "^16.1.6",
    "@angular/cli": "^16.2.9",
    "@angular/common": "^16.1.3",
    "@angular/compiler": "^16.1.3",
    "@angular/core": "^16.1.3",
    "@angular/fire": "^16.0.0",
    "@angular/forms": "^16.1.3",
    "@angular/platform-browser": "^16.1.3",
    "@angular/platform-browser-dynamic": "^16.1.3",
    "@angular/router": "^16.1.3",
    "chart.js": "^4.4.0",
    "google-auth-library": "^9.6.3",
    "google-spreadsheet": "^4.1.1",
    "gsap": "^3.12.2",
    "leaflet": "^1.9.4",
    "rxjs": "~7.8.0",
    "sass-resources-loader": "^2.2.5",
    "swiper": "^10.0.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.3",
    "@angular/compiler-cli": "^16.1.3",
    "@schematics/angular": "^16.1.3",
    "@types/jasmine": "~4.3.0",
    "@types/leaflet": "^1.9.8",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^16.1.0",
    "typescript": "~4.9.4"
  }
}
